import javax.swing.JComponent;


public class AbstractSpace extends JComponent 
{
	protected int xLoc;
	protected int yLoc;
	private static final int BLOCK_WIDTH = 600 / 15;
	private static final int BLOCK_HEIGHT = 400 / 10;
	public AbstractSpace(int x, int y)
	{
		xLoc = x * BLOCK_WIDTH;
		yLoc = y * BLOCK_HEIGHT;
	}
	

}
